<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.astroacharyamukti">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logonew"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logonew"
        android:supportsRtl="true"
        android:theme="@style/Theme.Astroacharyamukti">
        <activity
            android:name=".activity.HelpAndSupport"
            android:exported="false"
            android:label="Help and Supports" />
        <activity
            android:name=".activity.UserProfile"
            android:exported="false"
            android:label="User Profile" />
        <activity
            android:name=".activity.ReviewActivity"
            android:exported="false"
            android:label="Customer Review" />
        <activity
            android:name=".activity.MyEarningActivity"
            android:exported="false"
            android:label="My Earning" />
        <activity
            android:name=".activity.LanguageActivity"
            android:exported="true"
            android:label="Change Language"
            android:theme="@style/Theme.Astroacharyamukti.NoActionBar" />
        <activity
            android:name=".activity.PrivacyPolicy"
            android:exported="true"
            android:label="Privacy Policy" />
        <activity
            android:name=".activity.HomeActivity"
            android:exported="false"
            android:label="User Details"
            android:theme="@style/Theme.Astroacharyamukti.NoActionBar" />
        <activity
            android:name=".activity.Login"
            android:exported="false"
            android:theme="@style/Theme.Astroacharyamukti.NoActionBar" />
        <activity
            android:name=".activity.Launcher"
            android:exported="true"
            android:theme="@style/Theme.Design.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.BookAppointment"
            android:exported="false"
            android:label="Book Appointment" />
        <activity
            android:name=".activity.BankDetails"
            android:exported="true"
            android:label="Add Bank Details" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>